<h1>All tasks</h1>

<ul>
  <% @tasks.each do |task| %>
    <li style="list-style-type:none;">
      <%= check_box_tag "task_ids[]", task.id %>
      <%= link_to task.title, task_path(task), style: 'text-decoration: none;'  %>
    </li>
  <% end %>
</ul>

<p><%= link_to 'Create a task', tasks_new_path %></p>
